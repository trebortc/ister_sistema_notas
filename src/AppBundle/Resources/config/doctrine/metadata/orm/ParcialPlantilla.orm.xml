<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ParcialPlantilla" table="parcial_plantilla">
    <id name="idParcialPlantilla" type="integer" column="ID_PARCIAL_PLANTILLA">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="NOMBRE" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcion" type="string" column="DESCRIPCION" length="1024" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notaAprueba" type="decimal" column="NOTA_APRUEBA" precision="2" scale="2" nullable="true"/>
  </entity>
</doctrine-mapping>
